{
  "name": "@datastream/string",
  "version": "0.0.11",
  "description": "",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "engineStrict": true,
  "publishConfig": {
    "access": "public"
  },
  "main": "./index.web.mjs",
  "module": "./index.web.mjs",
  "exports": {
    ".": {
      "node": {
        "webstream": {
          "types": "./index.d.ts",
          "default": "./index.web.mjs"
        },
        "import": {
          "types": "./index.d.ts",
          "default": "./index.node.mjs"
        },
        "__require": {
          "types": "./index.d.ts",
          "default": "./index.node.cjs"
        }
      },
      "import": {
        "types": "./index.d.ts",
        "default": "./index.web.mjs"
      }
    }
  },
  "types": "index.d.ts",
  "files": [
    "*.web.mjs",
    "*.web.mjs.map",
    "*.node.mjs",
    "*.node.mjs.map",
    "*.node.cjs",
    "*.node.cjs.map",
    "*.d.ts"
  ],
  "scripts": {
    "test": "npm run test:unit",
    "test:unit": "ava",
    "test:benchmark": "node __benchmarks__/index.js"
  },
  "license": "MIT",
  "keywords": [
    "Web Stream API",
    "Node Stream API"
  ],
  "author": {
    "name": "datastream contributors",
    "url": "https://github.com/willfarrell/datastream/graphs/contributors"
  },
  "repository": {
    "type": "git",
    "url": "github:willfarrell/datastream",
    "directory": "packages/file-read"
  },
  "bugs": {
    "url": "https://github.com/willfarrell/datastream/issues"
  },
  "homepage": "https://datastream.js.org",
  "dependencies": {
    "@datastream/core": "0.0.11"
  },
  "gitHead": "c1774a4040720e50335008861b279b738e2e3ff1"
}
