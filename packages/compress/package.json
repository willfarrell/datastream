{
	"name": "@datastream/compress",
	"version": "0.0.42",
	"description": "Compression and decompression streams for gzip, deflate, brotli, and zstd",
	"type": "module",
	"engines": {
		"node": ">=24"
	},
	"engineStrict": true,
	"publishConfig": {
		"access": "public"
	},
	"main": "./index.web.mjs",
	"module": "./index.web.mjs",
	"exports": {
		".": {
			"node": {
				"import": {
					"types": "./index.d.ts",
					"default": "./index.node.mjs"
				},
				"__require": {
					"types": "./index.d.ts",
					"default": "./index.node.cjs"
				}
			},
			"import": {
				"types": "./index.d.ts",
				"default": "./index.web.mjs"
			}
		},
		"./webstream": {
			"types": "./index.d.ts",
			"default": "./index.web.mjs"
		},
		"./zstd": {
			"node": {
				"import": {
					"types": "./zstd.d.ts",
					"default": "./zstd.node.mjs"
				},
				"__require": {
					"types": "./zstd.d.ts",
					"default": "./zstd.node.cjs"
				}
			},
			"import": {
				"types": "./zstd.d.ts",
				"default": "./zstd.web.mjs"
			}
		},
		"./zstd/webstream": {
			"types": "./zstd.d.ts",
			"default": "./zstd.web.mjs"
		},
		"./brotli": {
			"node": {
				"import": {
					"types": "./brotli.d.ts",
					"default": "./brotli.node.mjs"
				},
				"__require": {
					"types": "./brotli.d.ts",
					"default": "./brotli.node.cjs"
				}
			},
			"import": {
				"types": "./brotli.d.ts",
				"default": "./brotli.web.mjs"
			}
		},
		"./brotli/webstream": {
			"types": "./brotli.d.ts",
			"default": "./brotli.web.mjs"
		},
		"./gzip": {
			"node": {
				"import": {
					"types": "./gzip.d.ts",
					"default": "./gzip.node.mjs"
				},
				"__require": {
					"types": "./gzip.d.ts",
					"default": "./gzip.node.cjs"
				}
			},
			"import": {
				"types": "./gzip.d.ts",
				"default": "./gzip.web.mjs"
			}
		},
		"./gzip/webstream": {
			"types": "./gzip.d.ts",
			"default": "./gzip.web.mjs"
		},
		"./deflate": {
			"node": {
				"import": {
					"types": "./deflate.d.ts",
					"default": "./deflate.node.mjs"
				},
				"__require": {
					"types": "./deflate.d.ts",
					"default": "./deflate.node.cjs"
				}
			},
			"import": {
				"types": "./deflate.d.ts",
				"default": "./deflate.web.mjs"
			}
		},
		"./deflate/webstream": {
			"types": "./deflate.d.ts",
			"default": "./deflate.web.mjs"
		},
		"./protobuf": {
			"node": {
				"import": {
					"types": "./protobuf.d.ts",
					"default": "./protobuf.node.mjs"
				},
				"__require": {
					"types": "./protobuf.d.ts",
					"default": "./protobuf.node.cjs"
				}
			},
			"import": {
				"types": "./protobuf.d.ts",
				"default": "./protobuf.web.mjs"
			}
		},
		"./protobuf/webstream": {
			"types": "./protobuf.d.ts",
			"default": "./protobuf.web.mjs"
		}
	},
	"types": "index.d.ts",
	"files": [
		"*.mjs",
		"*.cjs",
		"*.map",
		"*.d.ts"
	],
	"scripts": {
		"test": "npm run test:unit",
		"test:unit": "node --test",
		"test:benchmark": "node __benchmarks__/index.js"
	},
	"license": "MIT",
	"keywords": [
		"Web Stream API",
		"Node Stream API"
	],
	"author": {
		"name": "datastream contributors",
		"url": "https://github.com/willfarrell/datastream/graphs/contributors"
	},
	"repository": {
		"type": "git",
		"url": "github:willfarrell/datastream",
		"directory": "packages/compress"
	},
	"bugs": {
		"url": "https://github.com/willfarrell/datastream/issues"
	},
	"homepage": "https://datastream.js.org",
	"dependencies": {
		"@datastream/core": "0.0.42",
		"brotli-wasm": "3.0.1",
		"zstd-codec": "0.1.5"
	},
	"gitHead": "6ddc0fadabf5f3702a51aebae1fc6b252c6ae8d4"
}
